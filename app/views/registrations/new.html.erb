<div align="center">
  <h2>Sign Up</h2>
  <%= form_with model: @user, url: signup_path, html: {autocomplete: "off" } do |f| %>
    <%= f.label :name %><br>
    <%= f.text_field :name %><br><br>

    <%= f.label :email %><br>
    <%= f.email_field :email %><br><br>

    <%= f.submit "Sign Up" %>
  <% end %>
</div>
